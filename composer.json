{
    "name": "lewisjenkins/craft-color-picker",
    "description": "A simple color picker for Craft CMS.",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft color picker",
        "lj color picker"
    ],
    "support": {
        "docs": "https://github.com/lewisjenkins/craft-color-picker/blob/master/README.md",
        "issues": "https://github.com/lewisjenkins/craft-color-picker/issues",
        "email": "lewis@lj.io"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Lewis Jenkins",
            "homepage": "https://lj.io"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "php": "^8.0.2"
    },
    "autoload": {
        "psr-4": {
          "lewisjenkins\\craftcolorpicker\\": "src/"
        }
    },
    "extra": {
        "name": "LJ Color Picker",
        "handle": "craft-color-picker",
        "developer": "Lewis Jenkins",
        "developerUrl": "https://lj.io",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/lewisjenkins/craft-color-picker/master/CHANGELOG.md",
        "documentationUrl": "https://github.com/lewisjenkins/craft-color-picker/blob/master/README.md",
        "class": "lewisjenkins\\craftcolorpicker\\CraftColorPicker"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
